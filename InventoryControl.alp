			<Events>
				<Event>
					<Id>1700724050311</Id>
					<Name><![CDATA[generateDemand]]></Name>
					<X>40</X><Y>500</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Properties TriggerType="timeout" Mode="cyclic">
						<Timeout Class="CodeUnitValue">
							<Code><![CDATA[1]]></Code>
							<Unit Class="TimeUnits"><![CDATA[HOUR]]></Unit>
						</Timeout>
						<Rate Class="CodeUnitValue">
							<Code><![CDATA[1]]></Code>
							<Unit Class="RateUnits"><![CDATA[PER_HOUR]]></Unit>
						</Rate>
						<OccurrenceAtTime>true</OccurrenceAtTime>
						<OccurrenceDate>1700726400000</OccurrenceDate>
						<OccurrenceTime Class="CodeUnitValue">
							<Code><![CDATA[0]]></Code>
							<Unit Class="TimeUnits"><![CDATA[HOUR]]></Unit>
						</OccurrenceTime>
						<RecurrenceCode Class="CodeUnitValue">
							<Code><![CDATA[1]]></Code>
							<Unit Class="TimeUnits"><![CDATA[DAY]]></Unit>
						</RecurrenceCode>
						<Condition><![CDATA[false]]></Condition>
					</Properties>
					<Action><![CDATA[if (radio.getValue() == 0) {
	currentDay = (int) (time() / day() + 1) + "";
	if (Integer.parseInt(currentDay) > 5
			&& Integer.parseInt(currentDay) % 5 == 1) {
		countTimes = 0;
	}
	if ((currentDay + "").length() == 1) {
		currentDay = "0" + currentDay;
	}
	demand = (int) uniform(leftBound, rightBound);
	totalDemand += demand;
	int demand1 = demand + preShortage;
	if ((inventoryNum - demand1) >= 0) {// 当前库存可以满足需求
		preShortage = 0;
		inventoryNum -= demand1;
		totalStorage += inventoryNum;
		outCollection.add("于第" + currentDay + "天出库: "
				+ demand1 + "，当前库存为: " + inventoryNum);
		// Collections.reverse(outCollection);
		String[] texts = outCollection
				.toArray(new String[outCollection.size()]);
		// 冒泡排序
		boolean flag = false;
		for (int i = 0; i < texts.length - 1; i++) {
			for (int j = 0; j < texts.length - i - 1; j++) {
