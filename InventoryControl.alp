			}
		}
		listbox.setItems(texts);
		meetDemand += inventoryNum;
		inventoryNum = 0;
		// 发出补货请求
		if (onRoad == false) {
			onRoad = true;
			countTimes++;
			totalCost = totalCost + perOrderCost;
			totalOrder = totalOrder == 1000000000
					? 0
					: totalOrder;
