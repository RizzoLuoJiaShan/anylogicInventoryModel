				if (prev < next) {
					flag = true;
					String temp = texts[j];
					texts[j] = texts[j + 1];
					texts[j + 1] = temp;
				}
			}
			if (flag == false) {
				break;
			} else {
				flag = false;
			}
		}
		listbox.setItems(texts);
		meetDemand += demand;
		if (inventoryNum < safeInventoryNum
				&& onRoad == false) {
			// 发出补货
			onRoad = true;
			countTimes++;
			totalCost = totalCost + perOrderCost;
			// totalOrder+=bench;
